<UserControl x:Class="Sinboda.SemiAuto.View.MachineryDebug.PageView.MachineryDebugPageView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Sinboda.SemiAuto.View.MachineryDebug.PageView"
             xmlns:sin="clr-namespace:Sinboda.Framework.Control.Controls;assembly=Sinboda.Framework.Control"
             xmlns:MultiLang="clr-namespace:Sinboda.Framework.Common.ResourceExtensions;assembly=Sinboda.Framework.Common"
             xmlns:myStaticResource="clr-namespace:Sinboda.SemiAuto.Model.DatabaseModel.Resource;assembly=Sinboda.SemiAuto.Model"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="2*" />
            <ColumnDefinition Width="*"/>
        </Grid.ColumnDefinitions>

        <sin:SinTabControl Grid.Column="0" SelectedIndex="0">
            <sin:SinTabItem Header="{MultiLang:StringResource 0, DefaultValue=电机}">
                <sin:SinGrid  Grid.Row="0">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="2*"/>
                        <RowDefinition Height="3*"/>
                        <RowDefinition Height="2*"/>
                        <RowDefinition Height="2*"/>
                    </Grid.RowDefinitions>

                    <sin:SinGroupBox Grid.Row="0">
                        <sin:SinGroupBox.Header>
                            <Grid HorizontalAlignment="Stretch">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="auto"/>
                                    <ColumnDefinition Width="30"/>
                                    <ColumnDefinition Width="auto"/>
                                </Grid.ColumnDefinitions>

                                <TextBlock Text="{MultiLang:StringResource 0, DefaultValue=平台}" VerticalAlignment="Center" HorizontalAlignment="Left" Foreground="#414187"/>
                                <StackPanel HorizontalAlignment="Right" Orientation="Horizontal"  Grid.Column="2"  VerticalAlignment="Center" Height="30">
                                </StackPanel>
                            </Grid>
                        </sin:SinGroupBox.Header>
                        <sin:SinGrid>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="*" />
                                <RowDefinition Height="*" />
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*" />
                                <ColumnDefinition Width="*" />
                                <ColumnDefinition Width="*" />
                                <ColumnDefinition Width="*" />
                                <ColumnDefinition Width="*" />
                                <ColumnDefinition Width="*" />
                            </Grid.ColumnDefinitions>
                            <Button Grid.Column="0" Grid.Row="0" Command="{Binding PlatformResetCommand}" Content="{MultiLang:StringResource 0, DefaultValue=平台机械原点复位}" HorizontalAlignment="Center" VerticalAlignment="Center" />
                            <Button Grid.Column="1" Grid.Row="0" Command="{Binding PlatformResetLogicalCommand}" Content="{MultiLang:StringResource 0, DefaultValue=平台工作原点复位}" HorizontalAlignment="Center" VerticalAlignment="Center" />
                            <StackPanel Grid.Column="2" Grid.Row="0"  Orientation="Horizontal" HorizontalAlignment="Center">
                                <TextBlock Text="X:" Margin="5,20,0,0"/>
                                <sin:SinTextBox Width="150" Text="{Binding PosXaxis, UpdateSourceTrigger=PropertyChanged}" Margin="10,0,0,0" />
                            </StackPanel>
                            <StackPanel Grid.Column="3" Grid.Row="0"  Orientation="Horizontal" HorizontalAlignment="Center">
                                <TextBlock Text="Y:" Margin="5,20,0,0"/>
                                <sin:SinTextBox Width="150" Text="{Binding PosYaxis, UpdateSourceTrigger=PropertyChanged}" Margin="10,0,0,0" />
                            </StackPanel>
                            
                            <Button Grid.Column="4" Grid.Row="0" Command="{Binding PlatformMoveCommand}" Content="{MultiLang:StringResource 0, DefaultValue=平台移动}" HorizontalAlignment="Center" VerticalAlignment="Center" />
                            <Button Grid.Column="5" Grid.Row="0" Command="{Binding PlatformStopCommand}" Content="{MultiLang:StringResource 0, DefaultValue=平台停止}" HorizontalAlignment="Center" VerticalAlignment="Center" />
                            
                            <StackPanel Grid.Column="0" Grid.Row="1"  Orientation="Horizontal" HorizontalAlignment="Center">
                                <sin:SinComboBox  Width="70" Margin="10,0,0,0" ItemsSource="{Binding RackSouce}" SelectedItem="{Binding RackDish}" />
                                <TextBlock Text="-" Margin="5,20,0,0" />
                                <sin:SinComboBox  Width="70" Margin="10,0,0,0" ItemsSource="{Binding PosSouce}" SelectedItem="{Binding Position}" />
                            </StackPanel>
                            <Button Grid.Column="1" Grid.Row="1" Command="{Binding MoveCellCommand}" Content="{MultiLang:StringResource 0, DefaultValue=移动}" HorizontalAlignment="Center" VerticalAlignment="Center" />
                            <Button Grid.Column="2" Grid.Row="1" Command="{Binding SetCellCommand}" CommandParameter="1" Content="{MultiLang:StringResource 0, DefaultValue=设置点位1}" HorizontalAlignment="Center" VerticalAlignment="Center" />
                            <Button Grid.Column="3" Grid.Row="1" Command="{Binding SetCellCommand}" CommandParameter="2" Content="{MultiLang:StringResource 0, DefaultValue=设置点位2}" HorizontalAlignment="Center" VerticalAlignment="Center" />
                            <Button Grid.Column="4" Grid.Row="1" Command="{Binding SetCellCommand}" CommandParameter="3" Content="{MultiLang:StringResource 0, DefaultValue=设置点位3}" HorizontalAlignment="Center" VerticalAlignment="Center" />
                            <Button Grid.Column="5" Grid.Row="1" Command="{Binding CalCellCommand}" Content="{MultiLang:StringResource 0, DefaultValue=计算所有点}" HorizontalAlignment="Center" VerticalAlignment="Center" />
                        </sin:SinGrid>
                    </sin:SinGroupBox>
                    
                    <sin:SinGroupBox Grid.Row="1">
                        <sin:SinGroupBox.Header>
                            <Grid HorizontalAlignment="Stretch">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="auto"/>
                                    <ColumnDefinition Width="30"/>
                                    <ColumnDefinition Width="auto"/>
                                </Grid.ColumnDefinitions>

                                <TextBlock Text="{MultiLang:StringResource 0, DefaultValue=Z轴}" VerticalAlignment="Center" HorizontalAlignment="Left" Foreground="#414187"/>
                                <StackPanel HorizontalAlignment="Right" Orientation="Horizontal"  Grid.Column="2"  VerticalAlignment="Center" Height="30">
                                </StackPanel>
                            </Grid>
                        </sin:SinGroupBox.Header>
                        <sin:SinGrid>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="*" />
                                <RowDefinition Height="2*" />
                            </Grid.RowDefinitions>
                            <sin:SinGrid Grid.Row="0">
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="*" />
                                    <RowDefinition Height="10"/>
                                </Grid.RowDefinitions>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="auto"/>
                                    <ColumnDefinition Width="10"/>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="10"/>
                                    <ColumnDefinition Width="auto"/>
                                    <ColumnDefinition Width="10"/>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="10"/>
                                    <ColumnDefinition Width="auto"/>
                                    <ColumnDefinition Width="10"/>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="10"/>
                                    <ColumnDefinition Width="auto"/>
                                    <ColumnDefinition Width="10"/>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="10"/>
                                    <ColumnDefinition Width="auto"/>
                                    <ColumnDefinition Width="10"/>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="20"/>
                                </Grid.ColumnDefinitions>

                                <!--#region 第一个电机-->
                                <TextBlock Grid.Column="0" Grid.Row="0" VerticalAlignment="Center" HorizontalAlignment="Right" Text="{MultiLang:StringResource 0, DefaultValue=电机速度}" />
                                <TextBlock Grid.Column="4" Grid.Row="0" VerticalAlignment="Center" HorizontalAlignment="Right" Text="{MultiLang:StringResource 0, DefaultValue=加速度}" />
                                <TextBlock Grid.Column="8" Grid.Row="0" VerticalAlignment="Center" HorizontalAlignment="Right" Text="{MultiLang:StringResource 0, DefaultValue=位置}" />
                                <TextBlock Grid.Column="12" Grid.Row="0" VerticalAlignment="Center" HorizontalAlignment="Right" Text="{MultiLang:StringResource 0, DefaultValue=步数}" />
                                <!--<TextBlock Grid.Column="12" Grid.Row="0" VerticalAlignment="Center" HorizontalAlignment="Right" Text="{MultiLang:StringResource 0, DefaultValue=原点状态}" />-->


                                <sin:SinTextBox Grid.Column="2" Grid.Row="0" Text="{Binding ZaxisMotor.Speed, UpdateSourceTrigger=PropertyChanged}" />
                                <sin:SinTextBox Grid.Column="6" Grid.Row="0" Text="{Binding ZaxisMotor.Accel, UpdateSourceTrigger=PropertyChanged}" />
                                <sin:SinTextBox Grid.Column="10" Grid.Row="0" Text="{Binding PosZaxis, UpdateSourceTrigger=PropertyChanged}"/>
                                <sin:SinTextBox Grid.Column="14" Grid.Row="0" Text="{Binding StepZaxis, UpdateSourceTrigger=PropertyChanged}" />
                                <!--#endregion-->
                            </sin:SinGrid>
                           
                            <sin:SinGrid Grid.Row="1">
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="*" />
                                    <RowDefinition Height="10"/>
                                    <RowDefinition Height="*" />
                                    <RowDefinition Height="10"/>
                                </Grid.RowDefinitions>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="10"/>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="10"/>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="10"/>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="10"/>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="10"/>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="10"/>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="10"/>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="20"/>
                                </Grid.ColumnDefinitions>

                                <!--#region 第一个电机-->
                                <sin:SinButton Grid.Column="0" Grid.Row="0" Command="{Binding XimcSlowMoveCommand}" CommandParameter="{Binding ZaxisMotor}" Content="{MultiLang:StringResource 0, DefaultValue=慢速移动}" />
                                <sin:SinButton Grid.Column="2" Grid.Row="0" Command="{Binding XimcFastMoveCommand}" CommandParameter="{Binding ZaxisMotor}" Content="{MultiLang:StringResource 0, DefaultValue=快速移动}" />
                                <sin:SinButton Grid.Column="4" Grid.Row="0" Command="{Binding XimcRightAlwaysCommand}" CommandParameter="{Binding ZaxisMotor}" Content="{MultiLang:StringResource 0, DefaultValue=一直向上}" />
                                <sin:SinButton Grid.Column="6" Grid.Row="0" Command="{Binding XimcLeftAlwaysCommand}" CommandParameter="{Binding ZaxisMotor}" Content="{MultiLang:StringResource 0, DefaultValue=一直向下}" />
                                <sin:SinButton Grid.Column="8" Grid.Row="0" Command="{Binding XimcMoveRelativeCommand}" CommandParameter="{Binding ZaxisMotor}" Content="{MultiLang:StringResource 0, DefaultValue=相对移动}" />
                                <sin:SinButton Grid.Column="10" Grid.Row="0" Command="{Binding XimcWorkResetCommand}" CommandParameter="{Binding ZaxisMotor}" Content="{MultiLang:StringResource 0, DefaultValue=工作原点复位}" />
                                <sin:SinButton Grid.Column="12" Grid.Row="0" Command="{Binding XimcResetCommand}" CommandParameter="{Binding ZaxisMotor}" Content="{MultiLang:StringResource 0, DefaultValue=机械原点复位}" />
                                <sin:SinButton Grid.Column="14" Grid.Row="0" Command="{Binding XimcStopCommand}" CommandParameter="{Binding ZaxisMotor}" Content="{MultiLang:StringResource 0, DefaultValue=停止}" />
                                <sin:SinButton Grid.Column="0" Grid.Row="2" Command="{Binding XimcSetOriginCommand}" CommandParameter="{Binding ZaxisMotor}" Content="{MultiLang:StringResource 0, DefaultValue=定位原点}" />
                                <sin:SinButton Grid.Column="2" Grid.Row="2" Command="{Binding XimcSaveCommand}" CommandParameter="{Binding ZaxisMotor}" Content="{MultiLang:StringResource 0, DefaultValue=保存}" />
                                <!--#endregion-->
                            </sin:SinGrid>
                        </sin:SinGrid>
                        
                    </sin:SinGroupBox>
                    
                    <sin:SinGroupBox Grid.Row="2">
                        <sin:SinGroupBox.Header>
                            <Grid HorizontalAlignment="Stretch">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="auto"/>
                                    <ColumnDefinition Width="30"/>
                                    <ColumnDefinition Width="auto"/>
                                </Grid.ColumnDefinitions>

                                <TextBlock Text="{MultiLang:StringResource 0, DefaultValue=X轴}" VerticalAlignment="Center" HorizontalAlignment="Left" Foreground="#414187"/>
                                <StackPanel HorizontalAlignment="Right" Orientation="Horizontal"  Grid.Column="2"  VerticalAlignment="Center" Height="30">
                                </StackPanel>
                            </Grid>
                        </sin:SinGroupBox.Header>
                        <sin:SinGrid>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="*" />
                                <RowDefinition Height="*" />
                            </Grid.RowDefinitions>

                            <sin:SinGrid Grid.Row="0">
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="*" />
                                    <RowDefinition Height="10"/>
                                </Grid.RowDefinitions>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="auto"/>
                                    <ColumnDefinition Width="10"/>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="10"/>
                                    <ColumnDefinition Width="auto"/>
                                    <ColumnDefinition Width="10"/>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="10"/>
                                    <ColumnDefinition Width="auto"/>
                                    <ColumnDefinition Width="10"/>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="10"/>
                                    <ColumnDefinition Width="auto"/>
                                    <ColumnDefinition Width="10"/>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="10"/>
                                    <ColumnDefinition Width="auto"/>
                                    <ColumnDefinition Width="10"/>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="20"/>
                                </Grid.ColumnDefinitions>

                                <!--#region 第二个电机-->
                                <TextBlock Grid.Column="0" Grid.Row="0" VerticalAlignment="Center" HorizontalAlignment="Right" Text="{MultiLang:StringResource 0, DefaultValue=电机速率}" />
                                <TextBlock Grid.Column="4" Grid.Row="0" VerticalAlignment="Center" HorizontalAlignment="Right" Text="{MultiLang:StringResource 0, DefaultValue=方向}" />
                                <TextBlock Grid.Column="8" Grid.Row="0" VerticalAlignment="Center" HorizontalAlignment="Right" Text="{MultiLang:StringResource 0, DefaultValue=步数}" />
                                <TextBlock Grid.Column="12" Grid.Row="0" VerticalAlignment="Center" HorizontalAlignment="Right" Text="{MultiLang:StringResource 0, DefaultValue=位置}" />
                                <TextBlock Grid.Column="16" Grid.Row="0" VerticalAlignment="Center" HorizontalAlignment="Right" Text="{MultiLang:StringResource 0, DefaultValue=工作原点}" />

                                <sin:SinComboBox  Grid.Column="2" Grid.Row="0" HorizontalAlignment="Left" ItemsSource="{x:Static myStaticResource:EnumTypeResource.RateSource}" Width="150"
                                                  SelectedValue="{Binding MotorList[0].UseFastSpeed}" VerticalContentAlignment="Center" SelectedValuePath="Value" DisplayMemberPath="DisplayValue"/>
                                <sin:SinComboBox  Grid.Column="6" Grid.Row="0" HorizontalAlignment="Left" ItemsSource="{x:Static myStaticResource:EnumTypeResource.DirectionSource}" Width="150"
                                                  SelectedValue="{Binding MotorList[0].Dir}" VerticalContentAlignment="Center" SelectedValuePath="Value" DisplayMemberPath="DisplayValue"/>
                                <sin:SinTextBox Grid.Column="10" Grid.Row="0" Text="{Binding MotorList[0].Steps, UpdateSourceTrigger=PropertyChanged}" />
                                <sin:SinTextBox Grid.Column="14" Grid.Row="0" Text="{Binding PosXaxis, UpdateSourceTrigger=PropertyChanged}"/>
                                <sin:SinTextBox Grid.Column="18" Grid.Row="0" Text="{Binding OriginXaxis, UpdateSourceTrigger=PropertyChanged}"/>
                                <!--#endregion-->
                            </sin:SinGrid>

                            <sin:SinGrid Grid.Row="1">
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="*" />
                                    <RowDefinition Height="10"/>
                                </Grid.RowDefinitions>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="10"/>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="10"/>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="10"/>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="10"/>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="10"/>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="10"/>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="10"/>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="20"/>
                                </Grid.ColumnDefinitions>

                                <!--#region 第二个电机-->
                                <sin:SinButton Grid.Column="0" Grid.Row="0" Command="{Binding MoveRelateCommand}" CommandParameter="{Binding MotorList[0]}" Content="{MultiLang:StringResource 0, DefaultValue=相对移动}" />
                                <sin:SinButton Grid.Column="2" Grid.Row="0" Command="{Binding MoveAbsoluteCommand}" CommandParameter="{Binding MotorList[0]}" Content="{MultiLang:StringResource 0, DefaultValue=绝对移动}" />
                                <sin:SinButton Grid.Column="4" Grid.Row="0" Command="{Binding AlwaysCommand}" CommandParameter="{Binding MotorList[0]}" Content="{MultiLang:StringResource 0, DefaultValue=一直移动}" />
                                <sin:SinButton Grid.Column="6" Grid.Row="0" Command="{Binding ResetCommand}" CommandParameter="{Binding MotorList[0]}" Content="{MultiLang:StringResource 0, DefaultValue=复位机械原点}" />
                                <sin:SinButton Grid.Column="8" Grid.Row="0" Command="{Binding ResetLogicalCommand}" CommandParameter="{Binding MotorList[0]}" Content="{MultiLang:StringResource 0, DefaultValue=复位工作原点}" />
                                <sin:SinButton Grid.Column="10" Grid.Row="0" Command="{Binding StopCommand}" CommandParameter="{Binding MotorList[0]}" Content="{MultiLang:StringResource 0, DefaultValue=停止}" />
                                <sin:SinButton Grid.Column="12" Grid.Row="0" Command="{Binding SetOriginCommand}" CommandParameter="{Binding MotorList[0]}" Content="{MultiLang:StringResource 0, DefaultValue=定位原点}"/>
                                <sin:SinButton Grid.Column="14" Grid.Row="0" Command="{Binding SaveCommand}" CommandParameter="{Binding MotorList[0]}" Content="{MultiLang:StringResource 0, DefaultValue=保存}"/>
                                <!--#endregion-->
                            </sin:SinGrid>
                        </sin:SinGrid>
                    </sin:SinGroupBox>
                    
                    <sin:SinGroupBox Grid.Row="3">
                        <sin:SinGroupBox.Header>
                            <Grid HorizontalAlignment="Stretch">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="auto"/>
                                    <ColumnDefinition Width="30"/>
                                    <ColumnDefinition Width="auto"/>
                                </Grid.ColumnDefinitions>

                                <TextBlock Text="{MultiLang:StringResource 0, DefaultValue=Y轴}" VerticalAlignment="Center" HorizontalAlignment="Left" Foreground="#414187"/>
                                <StackPanel HorizontalAlignment="Right" Orientation="Horizontal"  Grid.Column="2"  VerticalAlignment="Center" Height="30">
                                </StackPanel>
                            </Grid>
                        </sin:SinGroupBox.Header>
                        <sin:SinGrid>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="*" />
                                <RowDefinition Height="*" />
                            </Grid.RowDefinitions>

                            <sin:SinGrid Grid.Row="0">
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="*" />
                                    <RowDefinition Height="10"/>
                                </Grid.RowDefinitions>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="auto"/>
                                    <ColumnDefinition Width="10"/>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="10"/>
                                    <ColumnDefinition Width="auto"/>
                                    <ColumnDefinition Width="10"/>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="10"/>
