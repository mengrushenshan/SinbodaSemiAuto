            </sin:SinTabItem>
            
            <sin:SinTabItem Header="{MultiLang:StringResource 0, DefaultValue=风扇}">
                <sin:SinGroupBox Grid.Row="1">
                    <sin:SinGroupBox.Header>
                        <Grid HorizontalAlignment="Stretch">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="auto"/>
                                <ColumnDefinition Width="30"/>
                                <ColumnDefinition Width="auto"/>
                            </Grid.ColumnDefinitions>

                            <TextBlock Text="{MultiLang:StringResource 0, DefaultValue=风扇0}" VerticalAlignment="Center" HorizontalAlignment="Left" Foreground="#414187"/>
                            <StackPanel HorizontalAlignment="Right" Orientation="Horizontal"  Grid.Column="2"  VerticalAlignment="Center" Height="30">
                            </StackPanel>
                        </Grid>
                    </sin:SinGroupBox.Header>
                    <sin:SinGrid>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="*" />
                            <RowDefinition Height="10"/>
                            <RowDefinition Height="*" />
                            <RowDefinition Height="10"/>
                            <RowDefinition Height="*" />
                            <RowDefinition Height="10"/>
                        </Grid.RowDefinitions>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="10"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="10"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="10"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="10"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="10"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="10"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="10"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="10"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="20"/>
                        </Grid.ColumnDefinitions>

                        <!--#region 第一个风扇-->
                        <TextBlock Grid.Column="0" Grid.Row="0" VerticalAlignment="Center" HorizontalAlignment="Right" Text="{MultiLang:StringResource 0, DefaultValue=风扇}" />
                        <TextBlock Grid.Column="4" Grid.Row="0" VerticalAlignment="Center" HorizontalAlignment="Right" Text="{MultiLang:StringResource 0, DefaultValue=频率}" />
                        <TextBlock Grid.Column="8" Grid.Row="0" VerticalAlignment="Center" HorizontalAlignment="Right" Text="{MultiLang:StringResource 0, DefaultValue=占空比}" />

                        <sin:SinTextBox Grid.Column="2" Grid.Row="0" Text="{Binding FanList[0].Id}" IsEnabled="False" />
                        <sin:SinTextBox Grid.Column="6" Grid.Row="0" Text="{Binding FanList[0].Frequency, UpdateSourceTrigger=PropertyChanged}" />
                        <sin:SinTextBox Grid.Column="10" Grid.Row="0" Text="{Binding FanList[0].DutyRatio, UpdateSourceTrigger=PropertyChanged}" />

                        <sin:SinButton Grid.Column="12" Grid.Row="0" Command="{Binding CtrlFanCommand}" CommandParameter="{Binding FanList[0]}" Content="{Binding Fan1ButtonText}" />
                        <sin:SinButton Grid.Column="14" Grid.Row="0" CommandParameter="{Binding FanList[0]}" Content="{MultiLang:StringResource 0, DefaultValue=设置}" IsEnabled="False" />
                        <!--#endregion-->

                        <!--#region 第二个风扇-->
                        <TextBlock Grid.Column="0" Grid.Row="2" VerticalAlignment="Center" HorizontalAlignment="Right" Text="{MultiLang:StringResource 0, DefaultValue=风扇}" />
                        <TextBlock Grid.Column="4" Grid.Row="2" VerticalAlignment="Center" HorizontalAlignment="Right" Text="{MultiLang:StringResource 0, DefaultValue=频率}" />
                        <TextBlock Grid.Column="8" Grid.Row="2" VerticalAlignment="Center" HorizontalAlignment="Right" Text="{MultiLang:StringResource 0, DefaultValue=占空比}" />

                        <sin:SinTextBox Grid.Column="2" Grid.Row="2" Text="{Binding FanList[1].Id}" IsEnabled="False" />
                        <sin:SinTextBox Grid.Column="6" Grid.Row="2" Text="{Binding FanList[1].Frequency, UpdateSourceTrigger=PropertyChanged}" />
                        <sin:SinTextBox Grid.Column="10" Grid.Row="2" Text="{Binding FanList[1].DutyRatio, UpdateSourceTrigger=PropertyChanged}" />

                        <sin:SinButton Grid.Column="12" Grid.Row="2" Command="{Binding CtrlFanCommand}" CommandParameter="{Binding FanList[1]}" Content="{Binding Fan1ButtonText}" />
                        <sin:SinButton Grid.Column="14" Grid.Row="2" CommandParameter="{Binding FanList[1]}" Content="{MultiLang:StringResource 0, DefaultValue=设置}" IsEnabled="False" />
                        <!--#endregion-->

                        <!--#region 第三个风扇-->
                        <TextBlock Grid.Column="0" Grid.Row="4" VerticalAlignment="Center" HorizontalAlignment="Right" Text="{MultiLang:StringResource 0, DefaultValue=风扇}" />
                        <TextBlock Grid.Column="4" Grid.Row="4" VerticalAlignment="Center" HorizontalAlignment="Right" Text="{MultiLang:StringResource 0, DefaultValue=频率}" />
                        <TextBlock Grid.Column="8" Grid.Row="4" VerticalAlignment="Center" HorizontalAlignment="Right" Text="{MultiLang:StringResource 0, DefaultValue=占空比}" />

                        <sin:SinTextBox Grid.Column="2" Grid.Row="4" Text="{Binding FanList[2].Id}" IsEnabled="False" />
                        <sin:SinTextBox Grid.Column="6" Grid.Row="4" Text="{Binding FanList[2].Frequency, UpdateSourceTrigger=PropertyChanged}" />
                        <sin:SinTextBox Grid.Column="10" Grid.Row="4" Text="{Binding FanList[2].DutyRatio, UpdateSourceTrigger=PropertyChanged}" />

                        <sin:SinButton Grid.Column="12" Grid.Row="4" Command="{Binding CtrlFanCommand}" CommandParameter="{Binding FanList[2]}" Content="{Binding Fan1ButtonText}" />
                        <sin:SinButton Grid.Column="14" Grid.Row="4" CommandParameter="{Binding FanList[2]}" Content="{MultiLang:StringResource 0, DefaultValue=设置}" IsEnabled="False" />
                        <!--#endregion-->

                    </sin:SinGrid>
                </sin:SinGroupBox>
            </sin:SinTabItem>

            <sin:SinTabItem Header="{MultiLang:StringResource 0, DefaultValue=激光器}">
                <sin:SinGroupBox Grid.Row="2">
                    <sin:SinGroupBox.Header>
                        <Grid HorizontalAlignment="Stretch">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="auto"/>
                                <ColumnDefinition Width="30"/>
                                <ColumnDefinition Width="auto"/>
                            </Grid.ColumnDefinitions>

                            <TextBlock Text="{MultiLang:StringResource 0, DefaultValue=激光器}" VerticalAlignment="Center" HorizontalAlignment="Left" Foreground="#414187"/>
                            <StackPanel HorizontalAlignment="Right" Orientation="Horizontal"  Grid.Column="2"  VerticalAlignment="Center" Height="30">
                            </StackPanel>
                        </Grid>
                    </sin:SinGroupBox.Header>

                    <sin:SinGrid>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="auto" />
                            <RowDefinition Height="*" />
                            <RowDefinition Height="10"/>
                        </Grid.RowDefinitions>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="10"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="10"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="10"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="10"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="10"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="10"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="10"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="20"/>
                        </Grid.ColumnDefinitions>

                        <!--#region 激光器-->
                        <TextBlock Grid.Column="0" Grid.Row="0" VerticalAlignment="Center" HorizontalAlignment="Right" Text="{MultiLang:StringResource 0, DefaultValue=激光器}" />
                        <TextBlock Grid.Column="4" Grid.Row="0" VerticalAlignment="Center" HorizontalAlignment="Right" Text="{MultiLang:StringResource 0, DefaultValue=频率}" />
                        <TextBlock Grid.Column="8" Grid.Row="0" VerticalAlignment="Center" HorizontalAlignment="Right" Text="{MultiLang:StringResource 0, DefaultValue=占空比}" />

                        <sin:SinTextBox Grid.Column="2" Grid.Row="0" Text="{Binding Voltage}" />
                        <sin:SinTextBox Grid.Column="6" Grid.Row="0" />
                        <sin:SinTextBox Grid.Column="10" Grid.Row="0" />

                        <Button Grid.Column="12" Grid.Row="0" Content="{MultiLang:StringResource 0, DefaultValue=打开}" Command="{Binding OpenLightCommand}" />
                        <Button Grid.Column="14" Grid.Row="0" Content="{MultiLang:StringResource 0, DefaultValue=关闭}" Command="{Binding CloseLightCommand}" />
                        <!--#endregion-->

                    </sin:SinGrid>
                </sin:SinGroupBox>
            </sin:SinTabItem>

            <sin:SinTabItem Header="{MultiLang:StringResource 0, DefaultValue=蜂鸣器}">
                <sin:SinGroupBox Grid.Row="3">
                    <sin:SinGroupBox.Header>
                        <Grid HorizontalAlignment="Stretch">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="auto"/>
                                <ColumnDefinition Width="30"/>
                                <ColumnDefinition Width="auto"/>
                            </Grid.ColumnDefinitions>

                            <TextBlock Text="{MultiLang:StringResource 0, DefaultValue=蜂鸣器}" VerticalAlignment="Center" HorizontalAlignment="Left" Foreground="#414187"/>
                            <StackPanel HorizontalAlignment="Right" Orientation="Horizontal"  Grid.Column="2"  VerticalAlignment="Center" Height="30">
                            </StackPanel>
                        </Grid>
                    </sin:SinGroupBox.Header>
                    <sin:SinGrid>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="*" />
                            <RowDefinition Height="10"/>
                        </Grid.RowDefinitions>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="10"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="10"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="10"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="10"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="10"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="10"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="20"/>
                        </Grid.ColumnDefinitions>

                        <!--#region 激光器-->
                        <TextBlock Grid.Column="0" Grid.Row="0" VerticalAlignment="Center" HorizontalAlignment="Right" Text="{MultiLang:StringResource 0, DefaultValue=蜂鸣器}" />
                        <TextBlock Grid.Column="4" Grid.Row="0" VerticalAlignment="Center" HorizontalAlignment="Right" Text="{MultiLang:StringResource 0, DefaultValue=频率}" />
                        <TextBlock Grid.Column="8" Grid.Row="0" VerticalAlignment="Center" HorizontalAlignment="Right" Text="{MultiLang:StringResource 0, DefaultValue=占空比}" />

                        <sin:SinTextBox Grid.Column="2" Grid.Row="0" />
                        <sin:SinTextBox Grid.Column="6" Grid.Row="0" />
                        <sin:SinTextBox Grid.Column="10" Grid.Row="0" />
                        <!--#endregion-->

                    </sin:SinGrid>
                </sin:SinGroupBox>
            </sin:SinTabItem>
            
            <sin:SinTabItem Header="{MultiLang:StringResource 0, DefaultValue=升级}">
                <sin:SinGroupBox Grid.Row="2">
                    <sin:SinGroupBox.Header>
                        <Grid HorizontalAlignment="Stretch">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="auto"/>
                                <ColumnDefinition Width="30"/>
                                <ColumnDefinition Width="auto"/>
                            </Grid.ColumnDefinitions>

                            <TextBlock Text="{MultiLang:StringResource 0, DefaultValue=主控板}" VerticalAlignment="Center" HorizontalAlignment="Left" Foreground="#414187"/>
                            <StackPanel HorizontalAlignment="Right" Orientation="Horizontal"  Grid.Column="2"  VerticalAlignment="Center" Height="30">
                            </StackPanel>
                        </Grid>
                    </sin:SinGroupBox.Header>
                    <sin:SinGrid>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="4*" />
                            <ColumnDefinition Width="10" />
                            <ColumnDefinition Width="*" />
                            <ColumnDefinition Width="10" />
                            <ColumnDefinition Width="*" />
                        </Grid.ColumnDefinitions>

                        <TextBox Grid.Column="0" Text="{Binding UpgradeFile}" Height="40" Width="600" />
                        <sin:SinButton Grid.Column="2" Command="{Binding BrowseCommand}" Content="{MultiLang:StringResource 0, DefaultValue=浏览}" />
                        <sin:SinButton Grid.Column="4" Command="{Binding UpgradeCommand}" Content="{MultiLang:StringResource 0, DefaultValue=升级}" />
                    </sin:SinGrid>
                </sin:SinGroupBox>
            </sin:SinTabItem>
        </sin:SinTabControl>

        <sin:SinGroupBox Grid.Column="1">
            <sin:SinGroupBox.Header>
                <Grid HorizontalAlignment="Stretch">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="auto"/>
                        <ColumnDefinition Width="30"/>
                        <ColumnDefinition Width="auto"/>
                    </Grid.ColumnDefinitions>

                    <TextBlock Text="{MultiLang:StringResource 0, DefaultValue=相机设置}" VerticalAlignment="Center" HorizontalAlignment="Left" Foreground="#414187"/>
                    <StackPanel HorizontalAlignment="Right" Orientation="Horizontal"  Grid.Column="2"  VerticalAlignment="Center" Height="30">
                    </StackPanel>
                </Grid>
            </sin:SinGroupBox.Header>
            <sin:SinGrid>
                <Grid.RowDefinitions>
                    <RowDefinition Height="2*" />
                    <RowDefinition Height="10" />
                    <RowDefinition Height="*" />
                </Grid.RowDefinitions>

                <Border Grid.Row="0" BorderBrush="#3f3f96" BorderThickness="1">
                    <Image x:Name="image" Width="512" Height="512" Stretch="Fill" Source="{Binding CameraSouce}" MouseDown="image_MouseDown"/>
                </Border>

                <sin:SinGrid Grid.Row="2">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="*" />
                        <RowDefinition Height="10" />
                        <RowDefinition Height="*" />
                        <RowDefinition Height="10" />
                        <RowDefinition Height="*" />
                    </Grid.RowDefinitions>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="*"/>
                        <ColumnDefinition Width="10"/>
                        <ColumnDefinition Width="*"/>
                        <ColumnDefinition Width="10"/>
                        <ColumnDefinition Width="*"/>
                        <ColumnDefinition Width="10"/>
                        <ColumnDefinition Width="*"/>
                    </Grid.ColumnDefinitions>

                    <TextBlock Grid.Row="0" Grid.Column="0" Text="{MultiLang:StringResource 0, DefaultValue=聚焦图像张数}" VerticalAlignment="Center" HorizontalAlignment="Center"/>
                    <sin:SinTextBox Grid.Row="0" Grid.Column="2" Text="{Binding FocusImageCount, UpdateSourceTrigger=PropertyChanged}" VerticalAlignment="Center" />
                    <Button Grid.Row="0" Grid.Column="4" Command="{Binding FocusCommand}" Content="{MultiLang:StringResource 0, DefaultValue=相机聚焦}" IsEnabled="{Binding IsCameraOpenEnable, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Center" VerticalAlignment="Center"/>

                    <Button Grid.Row="2" Grid.Column="0" Command="{Binding OpenAndCloseCommand}" Content="{Binding CameraButtonText}" IsEnabled="{Binding IsCameraOpenEnable, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Center" VerticalAlignment="Center" />
                    <Button Grid.Row="2" Grid.Column="2" Command="{Binding BigImageCommand}" Content="{MultiLang:StringResource 0, DefaultValue=大图展示}" HorizontalAlignment="Center" VerticalAlignment="Center" />
                    <Button Grid.Row="2" Grid.Column="4" Command="{Binding CameraInitCommand}" Content="{MultiLang:StringResource 0, DefaultValue=相机初始化}" IsEnabled="{Binding IsCameraInitEnable, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Center" VerticalAlignment="Center" />

                    <Button Grid.Row="4" Grid.Column="0" Command="{Binding SetRoiCommand}" Content="{MultiLang:StringResource 0, DefaultValue=设置ROI}" IsEnabled="{Binding IsCameraOpenEnable, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                    <Button Grid.Row="4" Grid.Column="2" Command="{Binding ReSetRoiCommand}" Content="{MultiLang:StringResource 0, DefaultValue=重置ROI}" IsEnabled="{Binding IsCameraOpenEnable, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                </sin:SinGrid>
            </sin:SinGrid>
        </sin:SinGroupBox>
    </Grid>
</UserControl>
