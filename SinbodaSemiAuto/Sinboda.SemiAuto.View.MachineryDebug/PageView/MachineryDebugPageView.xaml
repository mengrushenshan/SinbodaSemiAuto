<UserControl
    x:Class="Sinboda.SemiAuto.View.MachineryDebug.PageView.MachineryDebugPageView"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:MultiLang="clr-namespace:Sinboda.Framework.Common.ResourceExtensions;assembly=Sinboda.Framework.Common"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:Sinboda.SemiAuto.View.MachineryDebug.PageView"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:myStaticResource="clr-namespace:Sinboda.SemiAuto.Model.DatabaseModel.Resource;assembly=Sinboda.SemiAuto.Model"
    xmlns:sin="clr-namespace:Sinboda.Framework.Control.Controls;assembly=Sinboda.Framework.Control"
    d:DesignHeight="450"
    d:DesignWidth="800"
    mc:Ignorable="d">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="2*" />
            <ColumnDefinition Width="*" />
        </Grid.ColumnDefinitions>

        <sin:SinTabControl Grid.Column="0" SelectedIndex="0">
            <sin:SinTabItem Header="{MultiLang:StringResource 0, DefaultValue=电机}">
                <sin:SinGrid Grid.Row="0">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="2*" />
                        <RowDefinition Height="2.8*" />
                        <RowDefinition Height="2*" />
                        <RowDefinition Height="2*" />
                        <RowDefinition Height="1.3*" />
                    </Grid.RowDefinitions>

                    <sin:SinGroupBox Grid.Row="0">
                        <sin:SinGroupBox.Header>
                            <Grid HorizontalAlignment="Stretch">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="auto" />
                                    <ColumnDefinition Width="30" />
                                    <ColumnDefinition Width="auto" />
                                </Grid.ColumnDefinitions>

                                <TextBlock
                                    HorizontalAlignment="Left"
                                    VerticalAlignment="Center"
                                    Foreground="#414187"
                                    Text="{MultiLang:StringResource 0,
                                                                    DefaultValue=平台}" />
                                <StackPanel
                                    Grid.Column="2"
                                    Height="30"
                                    HorizontalAlignment="Right"
                                    VerticalAlignment="Center"
                                    Orientation="Horizontal" />
                            </Grid>
                        </sin:SinGroupBox.Header>
                        <sin:SinGrid>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="*" />
                                <RowDefinition Height="*" />
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*" />
                                <ColumnDefinition Width="*" />
                                <ColumnDefinition Width="*" />
                                <ColumnDefinition Width="*" />
                                <ColumnDefinition Width="*" />
                                <ColumnDefinition Width="*" />
                            </Grid.ColumnDefinitions>
                            <Button
                                Grid.Row="0"
                                Grid.Column="0"
                                HorizontalAlignment="Center"
                                VerticalAlignment="Center"
                                Command="{Binding PlatformResetCommand}"
                                Content="{MultiLang:StringResource 0,
                                                                   DefaultValue=平台机械原点复位}"
                                IsEnabled="{Binding IsIdle}" />
                            <Button
                                Grid.Row="0"
                                Grid.Column="1"
                                HorizontalAlignment="Center"
                                VerticalAlignment="Center"
                                Command="{Binding PlatformResetLogicalCommand}"
                                Content="{MultiLang:StringResource 0,
                                                                   DefaultValue=平台工作原点复位}"
                                IsEnabled="{Binding IsIdle}" />
                            <StackPanel
                                Grid.Row="0"
                                Grid.Column="2"
                                HorizontalAlignment="Center"
                                Orientation="Horizontal">
                                <TextBlock Margin="5,20,0,0" Text="X:" />
                                <sin:SinTextBox
                                    Width="150"
                                    Margin="10,0,0,0"
                                    Text="{Binding PosXaxis, UpdateSourceTrigger=PropertyChanged}" />
                            </StackPanel>
                            <StackPanel
                                Grid.Row="0"
                                Grid.Column="3"
                                HorizontalAlignment="Center"
                                Orientation="Horizontal">
                                <TextBlock Margin="5,20,0,0" Text="Y:" />
                                <sin:SinTextBox
                                    Width="150"
                                    Margin="10,0,0,0"
                                    Text="{Binding PosYaxis, UpdateSourceTrigger=PropertyChanged}" />
                            </StackPanel>

                            <Button
                                Grid.Row="0"
                                Grid.Column="4"
                                HorizontalAlignment="Center"
                                VerticalAlignment="Center"
                                Command="{Binding PlatformMoveCommand}"
                                Content="{MultiLang:StringResource 0,
                                                                   DefaultValue=平台移动}"
                                IsEnabled="{Binding IsIdle}" />
                            <Button
                                Grid.Row="0"
                                Grid.Column="5"
                                HorizontalAlignment="Center"
                                VerticalAlignment="Center"
                                Command="{Binding PlatformStopCommand}"
                                Content="{MultiLang:StringResource 0,
                                                                   DefaultValue=平台停止}"
                                IsEnabled="{Binding IsIdle}" />

                            <StackPanel
                                Grid.Row="1"
                                Grid.Column="0"
                                HorizontalAlignment="Center"
                                Orientation="Horizontal">
                                <sin:SinComboBox
                                    Width="70"
                                    Margin="10,0,0,0"
                                    ItemsSource="{Binding RackSouce}"
                                    SelectedItem="{Binding RackDish}" />
                                <TextBlock Margin="5,20,0,0" Text="-" />
                                <sin:SinComboBox
                                    Width="70"
                                    Margin="10,0,0,0"
                                    ItemsSource="{Binding PosSouce}"
                                    SelectedItem="{Binding Position}" />
                            </StackPanel>
                            <Button
                                Grid.Row="1"
                                Grid.Column="1"
                                HorizontalAlignment="Center"
                                VerticalAlignment="Center"
                                Command="{Binding PlatformAgingCommand}"
                                Content="{MultiLang:StringResource 0,
                                                                   DefaultValue=平台老化}"
                                IsEnabled="{Binding IsIdle}" />
                            <Button
                                Grid.Row="1"
                                Grid.Column="2"
                                HorizontalAlignment="Center"
                                VerticalAlignment="Center"
                                Command="{Binding AgingStopCommand}"
                                CommandParameter="1"
                                Content="{MultiLang:StringResource 0,
                                                                   DefaultValue=停止老化}" />
                            <Button
                                Grid.Row="1"
                                Grid.Column="3"
                                HorizontalAlignment="Center"
                                VerticalAlignment="Center"
                                Command="{Binding SetCellCommand}"
                                CommandParameter="2"
                                Content="{MultiLang:StringResource 0,
                                                                   DefaultValue=设置点位2}"
                                IsEnabled="{Binding IsIdle}" />
                            <Button
                                Grid.Row="1"
                                Grid.Column="4"
                                HorizontalAlignment="Center"
                                VerticalAlignment="Center"
                                Command="{Binding SetCellCommand}"
                                CommandParameter="3"
                                Content="{MultiLang:StringResource 0,
                                                                   DefaultValue=设置点位3}"
                                IsEnabled="{Binding IsIdle}" />
                            <Button
                                Grid.Row="1"
                                Grid.Column="5"
                                HorizontalAlignment="Center"
                                VerticalAlignment="Center"
                                Command="{Binding CalCellCommand}"
                                Content="{MultiLang:StringResource 0,
                                                                   DefaultValue=计算所有点}"
