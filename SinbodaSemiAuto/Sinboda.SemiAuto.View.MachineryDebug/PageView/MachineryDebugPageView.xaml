                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="10"/>
                                    <ColumnDefinition Width="auto"/>
                                    <ColumnDefinition Width="10"/>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="20"/>
                                </Grid.ColumnDefinitions>

                                <!--#region 第三个电机-->
                                <TextBlock Grid.Column="0" Grid.Row="0" VerticalAlignment="Center" HorizontalAlignment="Right" Text="{MultiLang:StringResource 0, DefaultValue=电机速率}" />
                                <TextBlock Grid.Column="4" Grid.Row="0" VerticalAlignment="Center" HorizontalAlignment="Right" Text="{MultiLang:StringResource 0, DefaultValue=方向}" />
                                <TextBlock Grid.Column="8" Grid.Row="0" VerticalAlignment="Center" HorizontalAlignment="Right" Text="{MultiLang:StringResource 0, DefaultValue=步数}" />
                                <TextBlock Grid.Column="12" Grid.Row="0" VerticalAlignment="Center" HorizontalAlignment="Right" Text="{MultiLang:StringResource 0, DefaultValue=位置}" />
                                <TextBlock Grid.Column="16" Grid.Row="0" VerticalAlignment="Center" HorizontalAlignment="Right" Text="{MultiLang:StringResource 0, DefaultValue=工作原点}" />

                                <sin:SinComboBox  Grid.Column="2" Grid.Row="0" HorizontalAlignment="Left" ItemsSource="{x:Static myStaticResource:EnumTypeResource.RateSource}" Width="150"
                                                  SelectedValue="{Binding MotorList[1].UseFastSpeed}" VerticalContentAlignment="Center" SelectedValuePath="Value" DisplayMemberPath="DisplayValue"/>
                                <sin:SinComboBox  Grid.Column="6" Grid.Row="0" HorizontalAlignment="Left" ItemsSource="{x:Static myStaticResource:EnumTypeResource.DirectionSource}" Width="150"
                                                  SelectedValue="{Binding MotorList[1].Dir}" VerticalContentAlignment="Center" SelectedValuePath="Value" DisplayMemberPath="DisplayValue"/>
                                <sin:SinTextBox Grid.Column="10" Grid.Row="0" Text="{Binding MotorList[1].Steps, UpdateSourceTrigger=PropertyChanged}" />
                                <sin:SinTextBox Grid.Column="14" Grid.Row="0" Text="{Binding PosYaxis, UpdateSourceTrigger=PropertyChanged}" />
                                <sin:SinTextBox Grid.Column="18" Grid.Row="0" Text="{Binding OriginYaxis, UpdateSourceTrigger=PropertyChanged}"/>
                                <!--#endregion-->
                            </sin:SinGrid>

                            <sin:SinGrid Grid.Row="1">
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="*" />
                                    <RowDefinition Height="10"/>
                                </Grid.RowDefinitions>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="10"/>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="10"/>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="10"/>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="10"/>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="10"/>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="10"/>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="10"/>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="20"/>
                                </Grid.ColumnDefinitions>

                                <!--#region 第三个电机-->
                                <sin:SinButton Grid.Column="0" Grid.Row="0" Command="{Binding MoveRelateCommand}" CommandParameter="{Binding MotorList[1]}" Content="{MultiLang:StringResource 0, DefaultValue=相对移动}" />
                                <sin:SinButton Grid.Column="2" Grid.Row="0" Command="{Binding MoveAbsoluteCommand}" CommandParameter="{Binding MotorList[1]}" Content="{MultiLang:StringResource 0, DefaultValue=绝对移动}" />
                                <sin:SinButton Grid.Column="4" Grid.Row="0" Command="{Binding AlwaysCommand}" CommandParameter="{Binding MotorList[1]}" Content="{MultiLang:StringResource 0, DefaultValue=一直移动}" />
                                <sin:SinButton Grid.Column="6" Grid.Row="0" Command="{Binding ResetCommand}" CommandParameter="{Binding MotorList[1]}" Content="{MultiLang:StringResource 0, DefaultValue=复位机械原点}" />
